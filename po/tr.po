"// UNSOUND. DO NOT DO THIS.\n"
"    /*\n"
"    let r: &i32 = unsafe { &*p1 };\n"
"    dbg!(r);\n"
"    x = 50;\n"
"    dbg!(r); // Object underlying the reference has been mutated. This is "
"UB.\n"
"    */"
