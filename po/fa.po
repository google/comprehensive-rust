"            age,\n"
"            height,\n"
"            visit_count: 0,\n"
"            last_blood_pressure: None,\n"
"        }\n"
"    }\n"
"\n"
"    pub fn name(&self) -> &str {\n"
"        &self.name\n"
"    }\n"
"\n"
"    pub fn age(&self) -> u32 {\n"
"        self.age\n"
"    }\n"
"\n"
"    pub fn height(&self) -> f32 {\n"
"        self.height\n"
"    }\n"
"\n"
"    pub fn doctor_visits(&self) -> u32 {\n"
"        self.visit_count as u32\n"
"    }\n"
"\n"
"    pub fn set_age(&mut self, new_age: u32) {\n"
"        self.age = new_age\n"
