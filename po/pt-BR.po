"0:  cmp x29, x30\n"
"    b.hs 1f\n"
"    stp xzr, xzr, [x29], #16\n"
"    b 0b\n"
"\n"
"1:  /* Prepare a pilha. */\n"
"    adr_l x30, boot_stack_end\n"
"    mov sp, x30\n"
"\n"
"    /* Configure o vetor de exce√ß√£o. */\n"
"    adr x30, vector_table_el1\n"
"    msr vbar_el1, x30\n"
"\n"
"    /* Chame o c√≥digo Rust. */\n"
"    bl main\n"
"\n"
"    /* Loop infinito esperando por interrup√ß√µes. */\n"
"2:  wfi\n"
"    b 2b\n"
"```"

